<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<!-- Adds all assets in Odoo -->
    <template id="assets_backend" name="asset management" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
             <!-- All links to CSS and JS files should be added in here. -->
             <link rel="stylesheet" href="/asset_management/static/src/scss/asset_management.scss"/>
             <script type="text/javascript" src="/asset_management/static/src/js/asset_management.js"></script>
        </xpath>
    </template>
</odoo>