    <!--depreciation run process form-->
    <record id="dep_run_process_form_view" model="ir.ui.view">
        <field name="name">dep run process form</field>
        <field name="model">asset_management.deprunprocess</field>
        <field name="arch" type="xml">
            <form string="Depreciation Run Process Form">
                <header>
                    <button name="reinstall" string="Reinstall" type="object" class="oe_highlight" attrs="{'invisible':['|',('id','=',False),
                                                            ('reinstall_flag','=',True)]}"/>
                </header>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="process_date"/>
                        </group>
                        <group>
                            <field name="process_period_id"/>
                            <field name="book_id"/>
                            <field name="reinstall_flag" invisible="1"/>
                        </group>
                    </group>
                    <notebook>
                        <page string="Depreciation Run Process lines">
                            <field name="dep_run_process_lines" widget="One2many_list" readonly="1"
                                   mode="tree,form">
                                <tree string="depreciation line" edit="false" create="false" delete="false">
                                    <field name="sequence"/>
                                    <field name="depreciation_id"/>
                                </tree>
                                <form>
                                    <sheet>
                                        <group>
                                            <group>
                                                <field name="sequence"/>
                                            </group>
                                            <group>
                                                <field name="dep_run_process_id"/>
                                                <field name="depreciation_id"/>
                                            </group>
                                        </group>
                                    </sheet>
                                </form>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <!--depreciation run process line form-->
    <record id="dep_run_process_line_form_view" model="ir.ui.view">
        <field name="name">dep run process line form view</field>
        <field name="model">asset_management.deprunprocess_line</field>
        <field name="arch" type="xml">
            <form name="Depreciation Run Process  line form ">
                <sheet>
                    <group>
                        <group>
                            <field name="sequence"/>
                        </group>
                        <group>
                            <field name="dep_run_process_id"/>
                            <field name="depreciation_id"/>
                        </group>
                    </group>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="activity_ids" widget="mail_activity"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
            </form>
        </field>
    </record>


