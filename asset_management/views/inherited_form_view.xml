<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_account_move_inherited_form" model="ir.ui.view">
        <field name="name">Account Move Form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='journal_id']" position="after">
                <field name="trx_type"/>
            </xpath>
            <xpath expr="//button[@name='173']" position="attributes">
                <attribute name="attrs">
                    {'invisible':['|','|',('trx_type','=','full_retirement'),('trx_type','=','partial_retirement'),('state','!=','posted')]}
                </attribute>
                <!--<button name="164" string="Reverse Entry" type="action" groups="account.group_account_invoice" attrs="{'invisible':['|','|',('trx_type','=','full_retirement'),('trx_type','=','partial_retirement'),('state','!=','posted')]}"/>-->
            
            </xpath>
        </field>

    </record>

</odoo>